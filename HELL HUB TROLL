                    fire.Parent = part
                end
            end
        end
    end
})

AvatarTab:AddButton({
    Name = "Skin Rainbow",
    Callback = function()
        local character = game.Players.LocalPlayer.Character
        coroutine.wrap(function()
            while true do
                for _, part in pairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                    end
                end
                task.wait(0.1)
            end
        end)()
    end
})

AvatarTab:AddButton({
    Name = "Gigante",
    Callback = function()
        local character = game.Players.LocalPlayer.Character
        if character then
            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") then
                    part.Size = part.Size * 2
                end
            end
            character.Humanoid.HipHeight = character.Humanoid.HipHeight * 2
        end
    end
})

AvatarTab:AddButton({
    Name = "Headless",
    Callback = function()
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChild("Head") then
            character.Head.Transparency = 1
            if character.Head:FindFirstChild("face") then
                character.Head.face:Destroy()
            end
        end
    end
})

AvatarTab:AddButton({
    Name = "Korblox",
    Callback = function()
        local character = game.Players.LocalPlayer.Character
        local rightLeg = character:FindFirstChild("Right Leg") or character:FindFirstChild("RightLowerLeg")
        if rightLeg then
            rightLeg.Transparency = 1
        end
    end
})


local selectedClothingId = ""

AvatarTab:AddTextBox({
    Name = "ID da Roupa",
    Placeholder = "Digite o ID da Shirt ou Pants",
    Callback = function(text)
        selectedClothingId = text
    end
})

AvatarTab:AddButton({
    Name = "Vestir Shirt",
    Callback = function()
        local char = game.Players.LocalPlayer.Character
        if char then
            -- Remove camisa anterior se houver
            local oldShirt = char:FindFirstChildOfClass("Shirt")
            if oldShirt then oldShirt:Destroy() end

            local newShirt = Instance.new("Shirt", char)
            newShirt.ShirtTemplate = "rbxassetid://" .. selectedClothingId
        end
    end
})

AvatarTab:AddButton({
    Name = "Vestir Pants",
    Callback = function()
        local char = game.Players.LocalPlayer.Character
        if char then
            -- Remove calça anterior se houver
            local oldPants = char:FindFirstChildOfClass("Pants")
            if oldPants then oldPants:Destroy() end

            local newPants = Instance.new("Pants", char)
            newPants.PantsTemplate = "rbxassetid://" .. selectedClothingId
        end
    end
})

local ScriptsTab = Window:MakeTab({"Scripts", ""})

local Paragraph = ScriptsTab:AddParagraph({"Scripts", "this tab is finished!!"})

ScriptsTab:AddButton({
    Name = "R4D HUB",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/R4D1R/Hub/main/Script.lua"))()
    end
})

ScriptsTab:AddButton({
    Name = "Fe doors",
    Callback = function()
    local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")

local LocalPlayer = Players.LocalPlayer
local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

mainStatus = true
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
	if input.KeyCode == Enum.KeyCode.RightControl and not gameProcessedEvent then
		mainStatus = not mainStatus
	end
end)

-- Ponto de referência invisível
local Folder = Instance.new("Folder", Workspace)
local Part = Instance.new("Part", Folder)
local Attachment1 = Instance.new("Attachment", Part)
Part.Anchored = true
Part.CanCollide = false
Part.Transparency = 1

-- Segue o HRP
RunService.Heartbeat:Connect(function()
	if humanoidRootPart then
		Part.Position = humanoidRootPart.Position
	end
end)

-- Network
if not getgenv().Network then
	getgenv().Network = {
		BaseParts = {},
		Velocity = Vector3.new(14.46262424, 14.46262424, 14.46262424)
	}

	Network.RetainPart = function(Part)
		if Part:IsA("BasePart") and Part:IsDescendantOf(Workspace) then
			if not table.find(Network.BaseParts, Part) then
				table.insert(Network.BaseParts, Part)
			end
			Part.CustomPhysicalProperties = PhysicalProperties.new(0, 0, 0, 0, 0)
			Part.CanCollide = false
			Part.Anchored = false
			Part.Massless = false
		end
	end

	local function EnablePartControl()
		LocalPlayer.ReplicationFocus = Workspace
		RunService.Heartbeat:Connect(function()
			sethiddenproperty(LocalPlayer, "SimulationRadius", math.huge)
			for _, Part in pairs(Network.BaseParts) do
				if Part:IsDescendantOf(Workspace) then
					Part.Velocity = Network.Velocity
					Part.CanCollide = false -- reforça
				end
			end
		end)
	end

	EnablePartControl()
end

-- Prevenir colisão com o personagem
local function PreventCollisionWithCharacter(part)
	for _, charPart in pairs(character:GetChildren()) do
		if charPart:IsA("BasePart") then
			local noCollision = Instance.new("NoCollisionConstraint")
			noCollision.Part0 = part
			noCollision.Part1 = charPart
			noCollision.Parent = part
		end
	end
end

-- Força e reativa parte
local function ForcePart(v)
	if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(character)
	and not v.Parent:FindFirstChildOfClass("Humanoid")
	and not v.Parent:FindFirstChild("Head") and v.Name ~= "Handle" then

		for _, x in ipairs(v:GetChildren()) do
			if x:IsA("BodyMover") or x:IsA("RocketPropulsion")
			or x:IsA("Weld") or x:IsA("WeldConstraint")
			or x:IsA("AlignPosition") or x:IsA("AlignOrientation")
			or x:IsA("Attachment") or x:IsA("Torque") then
				x:Destroy()
			end
		end

		v.Anchored = false
		v.CanCollide = false
		v.Massless = false

		Network.RetainPart(v)
		PreventCollisionWithCharacter(v)

		local Torque = Instance.new("Torque", v)
		Torque.Torque = Vector3.new(50000, 50000, 50000)

		local AlignPosition = Instance.new("AlignPosition", v)
		local Attachment2 = Instance.new("Attachment", v)

		Torque.Attachment0 = Attachment2

		AlignPosition.MaxForce = math.huge
		AlignPosition.MaxVelocity = math.huge
		AlignPosition.Responsiveness = 200
		AlignPosition.Attachment0 = Attachment2
		AlignPosition.Attachment1 = Attachment1

		task.spawn(function()
			while v and v.Parent and humanoidRootPart do
				if (v.Position - humanoidRootPart.Position).Magnitude > 50 then
					AlignPosition.Enabled = false
					task.wait(0.05)
					AlignPosition.Enabled = true
				end
				task.wait(1)
			end
		end)
	end
end

-- Aplica em tudo do mapa
for _, v in ipairs(workspace:GetDescendants()) do
	ForcePart(v)
end

-- Aplica em partes novas
workspace.DescendantAdded:Connect(function(v)
	task.wait(0.1)
	ForcePart(v)
end)

-- Reforço constante de CanCollide = false
task.spawn(function()
	while true do
		for _, part in ipairs(Network.BaseParts) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
		task.wait(0.5)
	end
end)

