        sound.SoundId = "rbxassetid://1843529274" -- som de festa
        sound.Looped = true
        sound:Play()

        coroutine.wrap(function()
            while true do
                game.Lighting.Ambient = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                wait(0.2)
            end
        end)()
    end
})

